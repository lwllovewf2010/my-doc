$.fn.occeFlyOut = function (options)
{
    //Defaults//
    var defaults = { background: '#EB3C00', height: '75px', width: '280px', foreground: '#FFFFFF', position: 'absolute', top: '175px', openRight: '0px', closedRight: '-234px', jSContent: null, hPContent: null, market: 'en-us', allMarkets: false, cookieDays: 14, cookieName: 'cntFlyOut' }
    //Cookie data//
    var opts = $.extend(defaults, options);
    var marketSpecific = $('html').attr('lang').toLowerCase();
    var hPLoad = $(opts.hPContent).html();
    //Check for the market option if it is true, run code. If allMarkets is set to true, show in all markets.//
    if (marketSpecific === opts.market || opts.allMarkets === true)
    {
        cookie_name = opts.cookieName;
        var occeCookieData;
        function getName()
        {
            if (document.cookie)
            {
                index = document.cookie.indexOf(cookie_name);
                if (index != -1)
                {
                    namestart = (document.cookie.indexOf("=", index) + 1);
                    nameend = document.cookie.indexOf(";", index);
                    if (nameend == -1) { nameend = document.cookie.length; }
                    occeCookieData = document.cookie.substring(namestart, nameend);
                    return occeCookieData;
                }
            }
        }
        var occeCookieData = getName();
        var occeFlyOutDate = new Date();
        occeFlyOutDate.setTime(occeFlyOutDate.getTime() + 1000 * 60 * 60 * 24 * opts.cookieDays);
        var occePageWidthFix = window.innerWidth;
        var occePageWidth = document.documentElement.clientWidth;
        var occePage = (occePageWidth || occePageWidthFix);
        var contentBegin = '<input class="occevalue" type="hidden"><div class="cntFlyOutContainer"><div class="cntPS1 cntFlyOut cntOCCEModHide"><span class="close"><img src="file.aspx?AssetId=ZA103157979" alt="" /></span><span class="open"><img alt="" src="file.aspx?AssetId=ZA103157981" /></span><div id="cntFlyOutInner">';
        var contentEnd = '</div><div class="cntHGC505050 cntFlyoutGreyBar" ><img src="file.aspx?AssetId=ZA104012834" alt="Try the Office Preview" /><a href="redir/XT103927624.aspx" class="btnSmall btnWhite cntPSRight">Try it<\/a><\/div></div></div>';
        if (opts.hPContent !== null && opts.jSContent === null) { $('body').append(contentBegin + hPLoad + contentEnd); }
        else if (opts.jSContent !== null && opts.hPContent === null) { $('body').append(contentBegin + opts.jSContent + contentEnd); }
        $('div.cntFlyOutContainer').css({ "overflow": "hidden", 'height': opts.height, 'width': opts.width, 'position': opts.position, 'top': opts.top, 'right': opts.openRight }).find('div.cntFlyOut').css({ 'background-color': opts.background, 'height': opts.height, 'width': opts.width, 'color': opts.foreground, 'position': opts.position, 'overflow': 'hidden' }).find('span.close, span.open').css({ 'float': 'left' }).find('span.open').hide();
        $('div.cntFlyOut').find('div#cntFlyOutInner').css({ 'margin-left': '56px' });
        //Hide the control if the initial screen size is too small to accomodate it.//
        var flyOutOpen = function ()
        {
            $('span.open').hide();
            $('div.cntFlyOut').css({ right: opts.openRight, height: "122px" }).removeClass('cntOCCEModHide');
            
            $('span.close').show();
        }
        var flyOutClose = function ()
        {
            $('span.close').hide();
            $('div.cntFlyOut').css({ right: opts.closedRight, height: "75px" }).removeClass('cntOCCEModHide');
            
            $('span.open').show();
        }
        var flyOutHidden = function ()
        {
            $('span.close').hide();
            $('div.cntFlyOut').css({ right: opts.closedRight, height: "75px" });
            
            $('span.open').show();
        }
        if (occePage <= 1015)
        {

            flyOutHidden();
        }
        else if (occePage < 1271 && occePage > 1016)
        {
            switch (occeCookieData)
            {
                case "Open":
                    flyOutClose();
                    break;
                case "Closed":
                    flyOutClose();
                    break;
                default:
                    flyOutClose();
                    break;
            }
        }
        else if (occePage > 1271)
        {
            switch (occeCookieData)
            {
                case "Open":
                    flyOutOpen();
                    break;
                case "Closed":
                    flyOutClose();
                    break;
                default:
                    flyOutClose();
                    break;
            }
        }
        else
        {
            //fail silently
        }
        $('span.close').click(function ()
        {
            document.cookie = opts.cookieName + "=Closed; expires=" + occeFlyOutDate;
            $(this).hide();
            $('div.cntFlyOut').animate({ right: opts.closedRight, height: "75px" });
            $('span.open').show();

        });
        $('span.open').click(function ()
        {
            document.cookie = opts.cookieName + "=Open; expires=" + occeFlyOutDate;
            $(this).hide();
            $('div.cntFlyOut').animate({ right: opts.openRight, height: "122px" });
            $('span.close').show();

        });
        $('input.occevalue').val(($(window).width()) - 940);
        $(window).resize(function ()
        {
            $('input.occevalue').val(($(window).width() - 940));
            var occeCookieData = getName();
            var availVal = $('input.occevalue').val();
            switch (occeCookieData)
            {
                case "Closed":
                    if (availVal < 75) { $(".cntFlyOut").hide(); }
                    else if (availVal > 75) { $(".cntFlyOut").removeClass('cntOCCEModHide').css({ height: "75px" }).show(); }
                    break;
                default:
                    if (availVal < 75) { $(".cntFlyOut").hide(); }
                    else if (availVal > 330) { $(".cntFlyOut").css({ right: opts.openRight, height: "122px" }).removeClass('cntOCCEModHide').show(); $('span.open').hide(); $('span.close').show(); }
                    else if (availVal < 330 || availVal > 75) { $(".cntFlyOut").css({ right: opts.closedRight, height: "75px" }).removeClass('cntOCCEModHide').show(); $('span.close').hide(); $('span.open').show(); }
                    break;
            }
        });
    }
    else
    {
        //fail silently
    }
};
//End FlyOut Plugin
