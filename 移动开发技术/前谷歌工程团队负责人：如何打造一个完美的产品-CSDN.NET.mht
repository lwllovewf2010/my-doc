@charset "utf-8";

body, div, label, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, p, blockquote, th, td{margin:0; padding:0; list-style:none; text-decoration:none;}
body{background:#fff; color:#333; font:normal 12px/24px Helvetica, Tahoma, Arial, sans-serif; text-decoration:none; text-align:center;}
input, checkbox, select, img{vertical-align:middle;}
label{vertical-align:middle; font-family:Tahoma;}
label input[type=checkbox]{vertical-align:middle;}
img{border:none;}
a{cursor:pointer;}
a:link,
a:visited{color:#333; text-decoration:none;}
a:hover,
a:active{color:#c00; text-decoration:none;}
label{_font-family:Tahoma;}
.tl{text-align:left;}
.tr{text-align:right;}
.tc{text-align:center;}
.fl{float:left;}
.fr{float:right;}

.wrap{width:960px; margin:0 auto; text-align:left;}

.head{height:54px; overflow:hidden;}
.head .logo{display:inline-block; float:left; width:93px; height:54px; background:url(../images/logo_csdn.png) no-repeat left center;}
.head .search{float:right; width:300px; height:24px; overflow:hidden; margin:14px 0 0; border:solid 1px #ccc;}
.head .search .sear_input{float:left; width:266px; *width:264px; height:24px; *height:22px; padding:0 5px; border:none; *border:solid 1px #fff; color:#333; font-size:12px; line-height:24px; *line-height:22px;}
.head .search .sear_btn{display:inline-block; float:left; width:24px; height:24px; overflow:hidden; border:none; background:url(../images/ico_search.gif) no-repeat center center; cursor:pointer;}

.nav{height:70px; overflow:hidden; margin:0 0 15px 0; background:url(../images/nav_bg.png) repeat-x top;}
.nav .cx_tabmenu{width:960px; margin:0 auto; text-align:left;}
.nav .lev_1{display:block; *display:inline-block; height:43px; overflow:hidden;}
.nav .lev_1:after{content:"."; display:block; height:0; clear:both; visibility:hidden;}
.nav .lev_1 li{float:left;}
.nav .lev_1 li a{display:inline-block; width:auto; height:38px; padding:0 25px 5px; color:#fff; font:bold 14px/38px Helvetica, Tahoma, Arial, sans-serif; text-decoration:none;}
.nav .lev_1 li a.current{padding:0 25px 5px !important; background:url(../images/arrow_nav_drop.gif) no-repeat center 0 !important;}
.nav .lev_1 li a:hover{padding:0 25px; background:url(../images/arrow_nav_drop.gif) no-repeat center 0;}

.nav .lev_2{display:block; *display:inline-block; height:27px; overflow:hidden; line-height:27px;}
.nav .lev_2:after{content:"."; display:block; height:0; clear:both; visibility:hidden;}
.nav .lev_2 div{display:none;}
.nav .lev_2 li{float:left;}
.nav .lev_2 li a{display:inline-block; height:27px; padding:0 25px; color:#333; text-decoration:none;}
.nav .lev_2 #sc2{padding-left:55px;}
.nav .lev_2 #sc3{padding-left:165px;}
.nav .lev_2 #sc4{padding-left:260px;}
.nav .lev_2 #sc5{padding-left:370px;}
.nav .lev_2 #sc6{padding-left:460px;}
